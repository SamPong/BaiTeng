#include <stdio.h>
#include <stdlib.h>

int main() {
	int begin, end, n, t;

	printf("请输入起始考场编号：");
	scanf("%d", &begin);

	while (begin < 0 || begin > 999) {
		printf("输入不合法，请重新输入起始考场编号：");
		scanf("%d", &begin);
	}
	printf("请输入末尾考场编号：");
	scanf("%d", &end);
	while (end < 0 || end > 999 || end < begin) {
		printf("输入不合法，请重新输入末尾考场编号：");
		scanf("%d", &end);
	}


	printf("请输入接龙人数(不包含接龙发起人)");
	scanf("%d", &n);
	getchar();
	if (end - begin + 1 == n)
		printf("已全部接龙！");
	else {
		char list[n][20];
		int  listnum[n];
		int i, j, flag;


		printf("请输入接龙列表：\n");


		for (i = 0; i < n; i++) {
			gets(list[i]);
		}


		for (i = 0; i < n; i++) {
			for (j = 0; j < 3; j++) {
				if (list[i][j] == '.') {
					listnum[i] = (list[i][j + 2] - '0') * 100 + (list[i][j + 3] - '0') * 10 + (list[i][j + 4] - '0');
					break;
				}
			}
		}
		printf("----------------\n");
		printf("未接龙的考场编号：\n");
		for (i = begin; i <= end; i++) {
			flag = 0;
			for (j = 0; j < n; j++) {
				if (i == listnum[j]) {
					flag = 1;
					break;
				}
			}
			if (flag == 0)
				printf("%d\n", i);
		}
	}

	return 0;
}


